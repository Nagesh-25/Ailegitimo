{
  "functions": {
    "app/app.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "source": "/analyze",
      "destination": "app/app.py"
    },
    {
      "source": "/chat",
      "destination": "app/app.py"
    },
    {
      "source": "/health",
      "destination": "app/app.py"
    },
    {
      "source": "/(.*)",
      "destination": "public/index.html"
    }
  ]
}
