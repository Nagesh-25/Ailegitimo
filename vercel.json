{
  "functions": {
    "api/analyze.py": {
      "runtime": "python3.9"
    },
    "api/chat.py": {
      "runtime": "python3.9"
    },
    "api/health.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/analyze",
      "dest": "/api/analyze.py"
    },
    {
      "src": "/api/chat",
      "dest": "/api/chat.py"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
